{
  "expo": {
    "name": "neovim",
    "slug": "neovim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "your-app-scheme",
    "primaryColor": "#ffffff",
    "backgroundColor": "#00000000",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/icon.png"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aryatechnologies.neovim"
    },
    "androidStatusBar": {},
    "androidNavigationBar": {},
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "76137339-c675-496f-87c2-5f0430bd25ad"
      }
    },
    "owner": "arya-technologies"
  }
}
